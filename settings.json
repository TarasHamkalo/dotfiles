{
  "workbench.colorTheme": "Default Light Modern",
  "redhat.telemetry.enabled": false,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "editor.unicodeHighlight.invisibleCharacters": false,
  "xml.server.vmargs": "-Xmx128M",
  "xml.symbols.maxItemsComputed": 50000,
  "files.autoSave": "afterDelay",
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingComments": "always",
  "editor.autoClosingQuotes": "never",
  "vim.leader": " ",
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["s-Tab"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["Tab"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },

    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },

    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },

    {
      "before": ["<leader>", "l", "f"],
      "commands": ["editor.action.formatDocument"]
    },

    {
      "before": ["<leader>", "l", "i"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<C-w>"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "r", "m"],
      "commands": ["workbench.action.closeOtherEditors"]
    }
  ],
  "git.confirmSync": false,
  "editor.snippets.codeActions.enabled": false,
  "editor.codeActionWidget.showHeaders": false,
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "dotnet.codeLens.enableTestsCodeLens": false,
  "editor.codeLens": false,
  "window.zoomLevel": 1,
  "vim.autoindent": true
}

